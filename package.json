{
  "private": "true",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vitepress build",
    "build-mpa": "vitepress build --mpa",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "vitepress dev",
    "postinstall": "husky install",
    "lint": "prettier  --write . && eslint --fix --ext .js,.ts,.vue",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:zh",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "markdownlint **/*.md && lint:zh",
    "lint:prettier": "prettier --check .",
    "lint:zh": "zhlint 'docs/**/*.md'",
    "serve": "vitepress serve",
    "site:preview": "vitepress build --outDir _dist",
    "up": "taze major -I"
  },
  "lint-staged": {
    "*.{js,ts,vue,scss,sass,css}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "prettier": "prettier-config-vuepress",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@iconify-json/ic": "^1.1.17",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/mdi": "^1.1.64",
    "@iconify-json/ph": "^1.1.11",
    "@mdit/plugin-container": "^0.8.0",
    "@mdit/plugin-figure": "^0.8.0",
    "@mdit/plugin-img-mark": "^0.8.0",
    "@mdit/plugin-img-size": "^0.8.0",
    "@mdit/plugin-mark": "^0.8.0",
    "@mdit/plugin-sub": "^0.8.0",
    "@mdit/plugin-sup": "^0.8.0",
    "@types/js-yaml": "^4.0.9",
    "@vueuse/core": "^10.7.2",
    "@vueuse/integrations": "^10.7.2",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "feed": "^4.2.2",
    "js-yaml": "^4.1.0",
    "markdown-it-footnote": "^4.0.0",
    "medium-zoom": "^1.1.0",
    "qrcode": "^1.5.3",
    "sass": "^1.71.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "vitepress": "1.0.0-rc.43",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/types": "^18.6.1",
    "@types/eslint": "^8.56.2",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unocss/eslint-config": "^0.58.5",
    "@vite-pwa/vitepress": "^0.3.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-vuepress": "^4.10.0",
    "eslint-config-vuepress-typescript": "^4.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "9.16.1",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markdown-it": "^14.0.0",
    "markdownlint-cli": "^0.38.0",
    "pnpm": "^8.15.3",
    "prettier": "^3.2.5",
    "prettier-config-vuepress": "^4.4.0",
    "sort-package-json": "^2.7.0",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "taze": "^0.13.3",
    "tsconfig-vuepress": "^4.5.0",
    "vite-plugin-inspect": "^0.8.3",
    "zhlint": "^0.7.4"
  },
  "packageManager": "pnpm@8.14.1"
}
